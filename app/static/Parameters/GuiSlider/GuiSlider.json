{
  "GuiSlider": {
    "ports": {
      "input": {
        "type": "input",
        "datatype": "any",
        "default": null
      },
      "max": {
        "type": "input",
        "datatype": "any",
        "default": 100
      },
      "min": {
        "type": "input",
        "datatype": "any",
        "default": 0
      },
      "initial_value": {
        "type": "input",
        "datatype": "any",
        "default": 0
      },
      "event": {
        "type": "output",
        "datatype": "any",
        "default": null
      },
      "self": {
        "type": "output",
        "datatype": "GuiSlider",
        "default": null
      }
    },
    "size": [
      85,
      60
    ],
    "constants": {
      "invert": {
        "type": "checkbox",
        "state": false,
        "help": "invert the output"
      },
      "output_float": {
        "type": "checkbox",
        "state": true,
        "help": "output value 0 to 1"
      }
    },
    "datatype": "int",
    "up": {
      "can_go_up": true,
      "type": "both",
      "order": 999
    },
    "broadcast": true
  }
}