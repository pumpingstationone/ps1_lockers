{
  "GuiLockerPicker": {
    "ports": {
      "input": {
        "type": "input",
        "datatype": "int",
        "default": null
      },
      "pod": {
        "type": "input",
        "datatype": "JSON",
        "default": false
      },
      "id_check": {
        "type": "input",
        "datatype": "any",
        "default": null
      },
      "event": {
        "type": "output",
        "datatype": "JSON",
        "default": null
      },
      "self": {
        "type": "output",
        "datatype": "GuiLockerPicker",
        "default": null
      }
    },
    "size": [
      85,
      40
    ],
    "datatype": "int",
    "up": {
      "can_go_up": true,
      "type": "both",
      "order": 999
    },
    "constants": {
      "heartbeat": {
        "type": "checkbox",
        "state": true,
        "help": "send state at regular interval"
      },
      "heartbeat_interval": {
        "type": "number",
        "state": 1000,
        "help": "interval to send state\nunits are in milliseconds"
      },
      "websocket": {
        "type": "text",
        "state": "",
        "help": "websocket is used when this is a standalone paramter running on pyscript"
      }
    },
    "broadcast": false
  }
}